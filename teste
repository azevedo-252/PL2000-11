PROGRAM logolissExemplo 
{
  DECLARATIONS 
    x = (300) , y -> Integer ;
    z -> Boolean ;
    w = FALSE -> Boolean ; 
    ww = "teste", yy -> String ;
  STATEMENTS
    SAY (h) ;
    SAY (x) ;
    SAY (ww) ;
    ASK ("qual é o numero?\n" , y ) ;
    SAY (y) ;
    ASK ("qual é a string?\n" , yy ) ;
    SAY (yy) ;	
    SAY ("IMPRIMIR O Y QUE DEVE TER 20\n") ;
    SAY (y);
}
